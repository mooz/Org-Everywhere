<?xml version="1.0" encoding="UTF-8"?>
<RDF:RDF xmlns:em="http://www.mozilla.org/2004/em-rdf#"
         xmlns:NC="http://home.netscape.com/NC-rdf#"
         xmlns:RDF="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
    <RDF:Description RDF:about="urn:mozilla:install-manifest"
                     em:id="org-everywhere@mooz.github.com"
                     em:name="Org Everywhere"
                     em:version="0.1"
                     em:creator="mooz"
                     em:description="Edit rich text editor with Org-mode notation"
                     em:homepageURL="https://github.com/mooz/org-everywhere"
                     em:optionsURL="about:blank?org-everywhere-config"
                     em:bootstrap="true">

        <em:updateURL></em:updateURL>
        <em:updateKey></em:updateKey>

        <em:localized>
            <RDF:Description em:locale="en-US"
                             em:name="Org Everywhere"
                             em:description="Edit rich text editor with Org-mode notation"
                             >
            </RDF:Description>
        </em:localized>

        <em:localized>
            <RDF:Description em:locale="ja-JP"
                             em:name="Org Everywhere"
                             em:description="リッチテキストエディタ内のコンテンツを Org 形式で編集"
                             >
            </RDF:Description>
        </em:localized>

        <!-- Firefox -->
        <em:targetApplication>
            <RDF:Description em:id="{ec8030f7-c20a-464f-9b0e-13a3a9e97384}"
                             em:minVersion="3.6"
                             em:maxVersion="9.*" />
        </em:targetApplication>
    </RDF:Description>
</RDF:RDF>
